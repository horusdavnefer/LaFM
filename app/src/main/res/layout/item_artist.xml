<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/cell"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/imagenArtist"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/splash_center"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"/>

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Sonata Artica"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@id/imagenArtist"/>

    <TextView
        android:id="@+id/listeners"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="838732873287"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        android:layout_toEndOf="@id/imagenArtist"
        android:layout_below="@+id/name"/>

    <TextView
        android:id="@+id/mbid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="ashhuasdhusadhusadhuisdauhi"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        android:layout_toEndOf="@id/imagenArtist"
        android:layout_below="@+id/listeners"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/darker_gray"
        android:layout_alignParentBottom="true"/>


</RelativeLayout>